<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.shape.Line?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Text?>
<Pane style="-fx-background-color: #182052;" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="controller.ProntuarioListaController">

    <Rectangle fill="#C5BAF4" height="600" width="913" x="237"/>

    <ImageView>
        <Image url="@../img/efeito-fundo-roxo-237.png"/>
    </ImageView>

    <Button layoutX="175" layoutY="25" mnemonicParsing="false" onAction="#navegarParaHome">
        <graphic>
            <ImageView fitHeight="35" fitWidth="40">
                <Image url="@../img/Duke.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Text styleClass="titulo" text="Paciente:" x="20" y="100"/>
    <Text styleClass="titulo" text="nome sobrenome" x="95" y="100"/>

    <Line endX="215" endY="135" startX="20" startY="135" stroke="white" strokeWidth="1.25"/>

    <Pane layoutX="20" layoutY="150" prefHeight="425" prefWidth="200" styleClass="painelPesquisa">
        <ListView fx:id="listViewDatas" layoutX="10" layoutY="60" prefHeight="350" prefWidth="180"/>
    </Pane>

    <Pane id="bordaInterna" layoutX="30" layoutY="165" prefHeight="30" prefWidth="180"
          styleClass="painelPesquisa"/>

    <TextField layoutX="35" layoutY="160" prefHeight="25" promptText="Pesquisar"/>

    <Button layoutX="165" layoutY="170" mnemonicParsing="false" prefHeight="15" prefWidth="40">
        <graphic>
            <ImageView fitHeight="20" fitWidth="20">
                <Image url="@../img/Duke.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Button layoutX="600" layoutY="150" mnemonicParsing="false" prefHeight="15" prefWidth="40"
            onAction="#navegarParaProntuarioEditar">
        <graphic>
            <ImageView fitHeight="200" fitWidth="200">
                <Image url="@../img/java_logo_256-256.png"/>
            </ImageView>
        </graphic>
    </Button>

    <Text text="NOVO PRONTUÁRIO" x="620" y="375"/>

</Pane>
