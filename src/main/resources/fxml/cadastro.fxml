<?import javafx.scene.text.Text?>

<Pane fx:id="rootPane" xmlns="http://javafx.com/javafx/17" xmlns:fx="http://javafx.com/fxml/1">
    <Rectangle fx:id="retanguloFundo" x="250" width="350" height="600" fill="#A38EFC"/>

    <Text fx:id="psiOrganize" x="50" y="400" text="PsiOrganize" styleClass="titulo"/>

