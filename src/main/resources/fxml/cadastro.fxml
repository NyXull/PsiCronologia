    <Line fx:id="linha1" startX="300" endX="550" startY="260" endY="260" stroke="black" strokeWidth="2"/>

    <Text fx:id="email" x="300" y="300" text="EMAIL" styleClass="label"/>
    <Text fx:id="emailFicticio" x="300" y="350" text="maria@gmail.com" styleClass="valor"/>

    <Line fx:id="linha2" startX="300" endX="550" startY="360" endY="360" stroke="black" strokeWidth="2"/>

    <Text fx:id="senha" x="300" y="400" text="SENHA" styleClass="label"/>
    <Text fx:id="senhaFicticia" x="300" y="450" text="**********" styleClass="valor"/>

    <Line fx:id="linha3" startX="300" endX="550" startY="460" endY="460" stroke="black" strokeWidth="2"/>

    <ImageView fx:id="imageView" x="75" y="250">
        <Image url="@../img/clipboard_resized.png"/>
    </ImageView>

</Pane>
